#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/dist/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/dist/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/dist/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/webfont@11.2.26_chokidar@3.6.0/node_modules/webfont/dist/cli.js" "$@"
fi
