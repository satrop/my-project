#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules/html-validate/bin/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules/html-validate/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules/html-validate/bin/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules/html-validate/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/html-validate@10.4.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/html-validate.mjs" "$@"
else
  exec node  "$basedir/../../bin/html-validate.mjs" "$@"
fi
