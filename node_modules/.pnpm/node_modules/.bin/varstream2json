#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules/varstream/cli/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules/varstream/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules/varstream/cli/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules/varstream/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/varstream@0.3.2/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../varstream/cli/varstream2json.js" "$@"
else
  exec node  "$basedir/../varstream/cli/varstream2json.js" "$@"
fi
