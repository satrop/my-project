#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/ttf2eot@2.0.0/node_modules/ttf2eot/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/ttf2eot@2.0.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/ttf2eot@2.0.0/node_modules/ttf2eot/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/ttf2eot@2.0.0/node_modules:/Users/stevenportas/Documents/Sites/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ttf2eot@2.0.0/node_modules/ttf2eot/ttf2eot.js" "$@"
else
  exec node  "$basedir/../../../../../ttf2eot@2.0.0/node_modules/ttf2eot/ttf2eot.js" "$@"
fi
